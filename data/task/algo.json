{
  "calculate_algo_metrics": {
    "base": "base",
    "run_on_start": ["get_metrics", "command_factory"],
    "arguments": {
      "command_name": "calculate_metric",
      "command_arg": "metric"
    }
  },
  "calculate_metric": {
    "base": "base",
    "dcn_task": true,
    "run_on_start": ["apply_limitations", "collect_normalization_data"],
