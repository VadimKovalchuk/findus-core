{
  "update_ticker_list": {
    "base": "base",
    "child_tasks": [
      "update_sp500_ticker_list",
      "update_sp400_ticker_list",
      "update_sp600_ticker_list"
    ],
    "run_on_done": ["new_tickers_processing"]
  },
  "update_sp500_ticker_list": {
    "base": "base",
    "dcn_task": true,
    "module": "findus-edge.tickers",
    "function": "get_sp500_ticker_list",
    "run_on_done": ["process_ticker_list"]
  },
  "update_sp400_ticker_list": {
    "base": "update_sp500_ticker_list",
    "function": "get_sp400_ticker_list"
  },
  "update_sp600_ticker_list": {
    "base": "update_sp500_ticker_list",
    "function": "get_sp600_ticker_list"
  },
  "append_daily_ticker_data": {
    "base": "base",
    "dcn_task": true,
    "run_on_start": ["define_ticker_daily_start_date"],
    "module": "findus-edge.yahoo",
    "function": "ticker_history",
    "run_on_done": ["validate_result_json", "append_prices", "append_dividends"]
  },
  "append_finviz_fundamental": {
    "base": "base",
    "dcn_task": true,
    "module": "findus-edge.finviz",
    "function": "fundamental_converted",
    "run_on_done": ["validate_result_json", "append_finviz_fundamental"]
  }
}
