{
  "update_ticker_list": {
    "base": "base",
    "child_tasks": [
      "update_sp500_ticker_list",
      "update_sp400_ticker_list",
      "update_sp600_ticker_list"
    ],
    "run_on_done": ["new_tickers_processing"]
  },
  "update_sp500_ticker_list": {
    "base": "base",
    "dcn_task": true,
    "module": "findus_edge.tickers",
    "function": "get_scope",
    "arguments": {"scope": "SP500"},
    "run_on_done": ["process_ticker_list"]
  },
  "update_sp400_ticker_list": {
    "base": "update_sp500_ticker_list",
    "arguments": {"scope": "SP400"}
  },
  "update_sp600_ticker_list": {
    "base": "update_sp500_ticker_list",
    "arguments": {"scope": "SP600"}
  },
  "get_new_ticker_data": {
    "base": "base",
    "run_on_start": ["clone_arguments_to_children"],
    "child_tasks": [
      "append_daily_ticker_data",
      "append_finviz_fundamental"
    ]
  },
  "collect_daily_global": {
    "base": "base",
    "run_on_start": ["get_all_tickers", "command_factory"],
    "arguments": {
      "command_name": "append_daily_ticker_data",
      "command_arg": "ticker"
    }
  },
  "append_daily_ticker_data": {
    "base": "base",
    "dcn_task": true,
    "run_on_start": ["define_ticker_daily_start_date"],
    "module": "findus_edge.yahoo",
    "function": "ticker_history",
    "run_on_done": ["validate_result_json", "append_prices", "append_dividends"]
  },
  "append_finviz_fundamental": {
    "base": "base",
    "dcn_task": true,
    "module": "findus_edge.finviz",
    "function": "fundamental_converted",
    "run_on_done": ["validate_result_json", "append_finviz_fundamental"]
  }
}
